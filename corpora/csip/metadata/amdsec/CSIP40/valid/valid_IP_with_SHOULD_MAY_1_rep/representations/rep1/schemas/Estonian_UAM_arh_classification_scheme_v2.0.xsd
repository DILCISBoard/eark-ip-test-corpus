<?xml version="1.0" encoding="utf-8"?>
<!--Created with Liquid XML Studio Developer Edition 9.0.11.3078 (http://www.liquid-technologies.com)-->
<!-- Rahvusarhiivi Universaalse Arhiveerimismooduli (UAM) väljundi arhiivikirjelduse XML skeem. -->
<xs:schema xmlns="http://www.ra.ee/public/Digiarhiiv/UAM/schemas/export/arhiiviskeem_eksport_EE_2.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.ra.ee/public/Digiarhiiv/UAM/schemas/export/arhiiviskeem_eksport_EE_2.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0 20.11.2013">
	<xs:element name="UAM_eksport_arhiivikirjeldus">
		<xs:annotation>
			<xs:documentation>Arhiivi ja projekti kirjeldus. Arhiivimoodustaja, kirjeldusüksuste (hierarhia), funktsioonide, projekti andmed</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Arhiivimoodustaja" type="ArhiivimoodustajaType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Arhiivimoodustaja kirjelduselemendid</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Projekt" type="ProjektType">
					<xs:annotation>
						<xs:documentation>Projekti kirjelduselemendid</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Kirjeldusyksus" type="KirjeldusyksusType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Kirjeldusüksuste (arhiiv, sari, toimik) kirjeldused</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Funktsioon" type="FunktsioonType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Funktsioonide ja volituste kirjeldused</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ArhiivimoodustajaType">
		<xs:annotation>
			<xs:documentation>Arhiivimoodustaja andmete tüüp</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AMArhiivIdent" type="xs:string">
				<xs:annotation>
					<xs:documentation>Arhiivi viitekood</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AMTyyp" type="AMTyypEnumerationType">
				<xs:annotation>
					<xs:documentation>Arhiivimoodustaja tüüp (moodustaja või allmoodustaja)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AMAsutusTyyp" type="AMAsutusTyypEnumerationType">
				<xs:annotation>
					<xs:documentation>Arhiivimoodustaja tüüp organisatsioonina (O) või eraisikuna  (I)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AMNimetus" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>(O) Organisatsiooni ametlik nimetus ning nimetuse muudatused koos kehtivusajaga. (I) Isiku täielik ees- ja perekonnanimi ning nimemuudatused koos kehtivusajaga</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Nimetus" type="xs:string">
							<xs:annotation>
								<xs:documentation>Arhiivimoodustaja nimevormi tekstiline väärtus</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Kehtivusaeg" type="AegType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Nimetuse, nime või nimemuutuse kehtivusaja algus ning lõpp</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Eesnimi" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Arhiivimoodustaja isiku puhul eesnimi</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Perekonnanimi" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Arhiivimoodustaja isiku puhul perekonnanimi</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="T2iend" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Arhiivimoodustaja isiku puhul nime t2iend</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMNimetusLyhend" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>(O) Organisatsiooni nimetuse üldkasutatav lühend või akronüüm koos kehtivusajaga. (I) Laialt kasutatav pseudonüüm või hüüdnimi koos kehtivusajaga.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Lyhend" type="xs:string">
							<xs:annotation>
								<xs:documentation>(O) Organisatsiooni nimetuse üldkasutatav lühend või akronüüm. (I) Laialt kasutatav pseudonüüm või hüüdnimi</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Kehtivusaeg" type="AegType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Lühendi, akronüümi, pseudonüümi või hüüdnime kehtivusaja algus ning lõpp</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMNimetusVoorkeelne" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>(O) Organisatsiooni ametlik nimetus võõrkeeles koos kehtivusajaga. (I) Isiku täielik nimi teistes tähestikes (näiteks ladina, slaavi, araabia) koos kehtivusajaga.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Voorkeelne" type="xs:string">
							<xs:annotation>
								<xs:documentation>(O) Organisatsiooni ametlik nimetus võõrkeeles. (I) Isiku täielik nimi teistes tähestikes.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Kehtivusaeg" type="AegType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Ametliku võõrkeelse nimetuse või teises tähestikus nime kehtivusaja algus ning lõpp</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Keel" type="xs:string" default="eesti" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Keel, milles ametlik võõrkeelne nimetus või teises tähestikus nimevorm esitatakse.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMRegistrikood" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>(O) Organisatsiooni registrikood riigi- ja kohaliku omavalitsuse asutuste riiklikus registris või äriregistris.	(I) Isikukood rahvastikuregistris</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AMRegister" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Registri nimi, millest pärineb organisatsiooni registrikood või isiku isikukood</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AMTegevusaeg" type="AegType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>(O) Aeg, millal organisatsioon on tegutsenud.	(I) Isiku sünni- ja surmadaatum</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AMAsukoht" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>(O) Asukoht, kus organisatsioon tegutses (viimane postiaadress)	(I) Koht, kus isik elas</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Lahiaadress" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Asukoha või elukoha maja nr, tänava nimetus, korteri nr</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Omavalitsusyksus" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Asukoha või elukoha linna, valla või sellega samaväärse ajaloolise administratiivüksuse nimetus (külanõukogu)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Asustusyksus" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Küla, vallasisene linn, alev jne</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Piirkond" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Vabatekstiline asukoha kirjeldus</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Riik" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Asukoha või elukoha riik</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SeosAeg" type="AegType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Arhiivimoodustaja ja asukoha või elukoha seose alg- ja lõppdaatum</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMStaatus" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>(O) Organisatsiooni staatus vastavalt riigi- ja kohaliku omavalitsuse asutuste riiklikule registrile või eraoigusliku organisatsioonina või muu staatusena koos kehtivusajaga	(I) Isiku rahvus</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Staatus" type="xs:string">
							<xs:annotation>
								<xs:documentation>(O) Organisatsiooni staatus vastavalt riigi- ja kohaliku omavalitsuse asutuste riiklikule registrile või eraoigusliku organisatsioonina või muu staatusena	(I) Isiku rahvus</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Kehtivusaeg" type="AegType">
							<xs:annotation>
								<xs:documentation>Organisatsiooni staatuse kehtivusaja algus ja lõpp</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMTegevusala" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>(O) Organisatsiooni dokumentide liigitusskeemis või  tegevust reguleerinud õigusaktides sätestatud põhifunktsioonid, tegevused või tegevusvaldkonnad koos volitusnormi ja daatumite äranäitamisega. 	(I) Isiku ametialad, ametinimetused, tegevusalad või ülesanded, vajadusel koos daatumitega</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Nimetus" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Funktsiooni, tegevuse, valdkonna, ametiala, ametikoha, tegevusala, ülesande jms nimetus</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Kirjeldus" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Liigitusskeemis oleva funktsiooni kirjeldus väljendatuna protsesside või tegevuste nimedena, mida funktsiooni täitmiseks teostatakse</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AMVolitused" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Normdokumendi liik, millega volitus on antud ja viide</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="VolitusTyyp" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Normdokumendi liik, millega volitus on antud</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="VolitusNimetus" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Normdokument, millega volitus on antud</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="VolitusAeg" type="AegType">
										<xs:annotation>
											<xs:documentation>Volituse kehtivusaja algus ja lõpp</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMStruktuur" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>(O) Organisatsiooni struktuuri ja selle muutumise esitus struktuuripuuna (lisafail) koos kehtivusaja daatumitega.(I) Eraisiku sugupuu (lisafail)
                    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Struktuur" type="FailType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Arhiivimoodustaja struktuur või sugupuu failina/pildina</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="KehtivusAeg" type="AegType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Struktuuriüksuse loomise aeg</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="StruktuurMuudatus" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Ühe perioodi jooksul toimunud muudatus</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Muudatus" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Ühe (struktuuripuuga esitatud) perioodi jooksul toimunud muudatus</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="JoustumineAeg" type="LaiendatudKuupaevType">
										<xs:annotation>
											<xs:documentation>Daatum, millal kirjeldatud struktuurimuudatus jõustus</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMAjalugu" type="xs:string">
				<xs:annotation>
					<xs:documentation>(O) Organisatsiooni ajaloo kokkuvõtlik esitus. (I) Isiku eluloo kokkuvõtlik esitus. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AMSeos" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Seonduva arhiivimoodustaja nimetus või nimi, koos seose kirjelduse ja daatumiga</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SeosNimi" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Seonduva arhiivimoodustaja ametlik nimetus või täielik ees- ja perekonnanimi</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SeosAeg" type="AegType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Seonduva ja kirjeldatava arhiivimoodustaja vahelise seose algus ja lõpp</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SeosSelgitus" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Selgitus loodud seose kohta, mis lühidalt täpsustab hierarhilist-, perekondlikku-, järgnevus- või mingit muud seost või annab seose kirjelduse narratiivina</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMPublikatsioon" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Infoallikas või publikatsioon, mis sisaldab teavet arhiivimoodustaja ajaloo, eluloo või mistahes muu olulise konteksti kohta</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Nimi" type="xs:string" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMKoostamine" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Kirjelduse koostamise või muutmise aeg ning koostaja(te) või muutja(te) nimi (nimed)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Nimi" type="xs:string">
							<xs:annotation>
								<xs:documentation>Kirjelduse koostaja nimi</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Aeg" type="LaiendatudKuupaevType">
							<xs:annotation>
								<xs:documentation>Kirjelduse koostamise aeg</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Tegevus" type="KoostamineTegevusEnumerationType">
							<xs:annotation>
								<xs:documentation>Valikuliselt: loomine, täiendamine</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AMKasutatudAllikad" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Arhiivimoodustaja kirjeldamiseks kasutatud kirjanduse loetelu</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Nimi" type="xs:string" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ProjektType">
		<xs:annotation>
			<xs:documentation>Projekti andmete tüüp</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ProjektNimi" type="xs:string">
				<xs:annotation>
					<xs:documentation>Projekti nimetus</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProjektAutor" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Projekti koostaja/autori ees- ja perenimi</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AutorAmet" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Autori ametikoht</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AutorAsutus" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Asutuse nimetus (autori asutus)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KoostamineKoht" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Koostamise koht</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NimistuNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Nimistu nr</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EsimeneSailikNumber" type="xs:nonNegativeInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Säilikute nummerdamise algus</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LoomineKuupaev" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Loomiskuupäev/sisendkuupäev</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EdastusKuupaev" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Edastuskuupäev</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EdastusKirjeldus" type="xs:string">
				<xs:annotation>
					<xs:documentation>Edastuse kirjeldus</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProjektTunnusArhiivis" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Projekti tunnus (ID) Rahvusarhiivi infosüsteemis</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KirjeldusyksusType">
		<xs:annotation>
			<xs:documentation>Kirjeldusüksuse andmete tüüp</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="KyTasand" type="KyTasandEnumerationType">
				<xs:annotation>
					<xs:documentation>Kirjeldusüksuse tasand (arhiiv, allarhiiv, sari, allsari, säilik, arhivaal) </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyIdentiteediala" type="KyIdentiteedialaType">
				<xs:annotation>
					<xs:documentation>Kirjeldusüksust identifitseerivad metaandmed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KySisuStruktAla" type="KySisuStruktAlaType">
				<xs:annotation>
					<xs:documentation>Kirjeldusüksuse sisu ja struktuuri metaandmed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KyJuurdepaasuala" type="JuurdepaasualaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Kirjeldusüksuse kasutamise piirangut kirjeldavad metaandmed</xs:documentation>
				</xs:annotation>
